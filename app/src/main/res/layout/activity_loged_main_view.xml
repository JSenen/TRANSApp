<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.LogedMainActivityView">

    <LinearLayout
        android:layout_width="390dp"
        android:layout_height="460dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/vista_logo"
            android:layout_width="match_parent"
            android:layout_height="241dp"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/logo" />


        <com.google.android.material.button.MaterialButton
            android:id="@+id/logedbut_ver_lineas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:onClick="logedModLines"
            android:text="Modificar Lineas"
            app:backgroundTint="@color/my_button_color"
            app:cornerRadius="8dp"
            app:icon="@android:drawable/ic_menu_edit"
            app:strokeColor="@color/my_button_stroke_color"
            app:strokeWidth="2dp" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>